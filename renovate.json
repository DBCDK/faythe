{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": false,
  "packageRules": [
    {
      "groupName": "Minor/Patch",
      "matchUpdateTypes":[
        "minor",
        "patch"
      ],
      "automerge":true
    },
    {
      "groupName": "Pin/Digest/PinDigest/LockFileMaintenance",
      "matchUpdateTypes":[
        "pin",
        "digest",
        "pinDigest",
        "lockFileMaintenance"
      ],
      "automerge":false
    }
  ]
}
